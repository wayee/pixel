package mm.pixel{	import flash.display.BitmapData;	import flash.display.DisplayObject;	import flash.events.EventDispatcher;	import flash.geom.Matrix;	import flash.geom.Point;	import flash.geom.Rectangle;	import flash.utils.getQualifiedClassName;		import mm.pixel.utils.PxFrameInfo;	import mm.pixel.utils.PxG;	import mm.pixel.utils.PxU;
	/**	 * 类似Sprite 	 * @author Andy Cai <huayicai@gmail.com>	 * 	 */	public class PxSprite extends PxObject	{		public function PxSprite( displayObject:DisplayObject )		{  			parseClip( displayObject );						x		= displayObject.x;			y		= displayObject.y;						super();		}				protected function parseClip( displayObject:DisplayObject ):void		{			var bounds:Rectangle = displayObject.getBounds(displayObject);			var id:String = getQualifiedClassName( displayObject ) + "_" + 1;			var frameInfo:PxFrameInfo;							if ( PxG.pxCollection.search( id ) )			{				frameInfo = PxG.pxCollection.collection[ id ];				bitmapData = frameInfo.bitmapData;			}			else			{				frameInfo = PxU.cacheBitmap(displayObject, true, 0x0);								bitmapData = PxG.pxCollection.addBitmapData( id, frameInfo ).bitmapData;			}		}	}}